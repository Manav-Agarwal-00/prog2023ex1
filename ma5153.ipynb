      "source": [
        "def set_intersection(list1, list2):\n",
        "  ilist = []\n",
        "\n",
        "  for x in list1:\n",
        "    for y in list2:\n",
        "      if x == y:\n",
        "        if x not in ilist:\n",
        "            ilist.append(x)\n",
        "  return ilist"
      ]
    },
